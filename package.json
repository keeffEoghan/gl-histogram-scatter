{
  "name": "@epok.tech/gl-histogram-scatter",
  "version": "1.0.2",
  "description": "Custom histograms using scatter approach in WebGL - BYORenderer (based on `regl`).",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keeffEoghan/gl-histogram-scatter.git"
  },
  "author": "Eoghan O'Keeffe <epok.tech@gmail.com> (https://epok.tech)",
  "license": "MIT",
  "engines": {
    "node": ">= 12.14.1",
    "yarn": ">= 1.22.0"
  },
  "scripts": {
    "start": "yarn run dev",
    "dev": "parcel --log-level verbose ./example/index.html ./example/assets/**/*",
    "docs": "rm -rf docs/ && parcel build ./example/index.html ./example/assets/**/* -d ./docs/ --public-url ./"
  },
  "dependencies": {
    "@epok.tech/array-utils": "^0.0.2",
    "@epok.tech/gl-screen-triangle": "^1.1.3",
    "glsl-luma": "^1.0.1",
    "glsl-map": "^1.0.1"
  },
  "devDependencies": {
    "glsl-noise": "^0.0.0",
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.1",
    "parcel": "^1.12.4",
    "regl": "^1.3.13"
  },
  "bugs": {
    "url": "https://github.com/keeffEoghan/gl-histogram-scatter/issues"
  },
  "homepage": "https://github.com/keeffEoghan/gl-histogram-scatter#readme",
  "keywords": [
    "webgl",
    "regl",
    "gpgpu",
    "postprocessing",
    "glsl"
  ],
  "directories": {
    "doc": "docs",
    "example": "example"
  }
}
